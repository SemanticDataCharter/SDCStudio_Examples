@prefix : <https://semanticdatacharter.com/ns/sdc4#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdc4: <https://semanticdatacharter.com/ns/sdc4/> .
@base <https://semanticdatacharter.com/ns/sdc4#> .

<https://semanticdatacharter.com/ns/sdc4/> rdf:type owl:Ontology ;
                                                rdfs:comment "This work is Copyright 2016 - 2026 Axius-SDC, Inc., All Rights Reserved. This ontology represents the Semantic Data Charter (SDC4) Reference Model (RM). It defines the classes and properties for building SDC4-compliant data models."@en-us ;
                                                rdfs:label "Semantic Data Charter (SDC4) Release 4.0.0; Reference Model Ontology"@en-us ;
                                                owl:versionInfo "4.0.0" .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#duration
xsd:duration rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gDay
xsd:gDay rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gMonth
xsd:gMonth rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gMonthDay
xsd:gMonthDay rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYearMonth
xsd:gYearMonth rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#time
xsd:time rdf:type rdfs:Datatype .


###  https://semanticdatacharter.com/ns/sdc4/BooleanListSimpleType
sdc4:BooleanListSimpleType rdf:type rdfs:Datatype ;
                           rdfs:comment "A simple type for a white space-delimited list of boolean values." .


###  https://semanticdatacharter.com/ns/sdc4/DecimalListSimpleType
sdc4:DecimalListSimpleType rdf:type rdfs:Datatype ;
                           rdfs:comment "A simple type for a white space-delimited list of decimal values." .


###  https://semanticdatacharter.com/ns/sdc4/DoubleListSimpleType
sdc4:DoubleListSimpleType rdf:type rdfs:Datatype ;
                          rdfs:comment "A simple type for a white space-delimited list of double precision values." .


###  https://semanticdatacharter.com/ns/sdc4/IntegerListSimpleType
sdc4:IntegerListSimpleType rdf:type rdfs:Datatype ;
                           rdfs:comment "A simple type for a white space-delimited list of integer values." .


###  https://semanticdatacharter.com/ns/sdc4/NonNegativeIntegerListSimpleType
sdc4:NonNegativeIntegerListSimpleType rdf:type rdfs:Datatype ;
                                      rdfs:comment "A simple type for a white space-delimited list of non-negative integer values." .


###  https://semanticdatacharter.com/ns/sdc4/PositiveIntegerListSimpleType
sdc4:PositiveIntegerListSimpleType rdf:type rdfs:Datatype ;
                                   rdfs:comment "A simple type for a white space-delimited list of positive integer values." .


###  https://semanticdatacharter.com/ns/sdc4/StringListSimpleType
sdc4:StringListSimpleType rdf:type rdfs:Datatype ;
                          rdfs:comment "A simple type for a white space-delimited list of string values." .


###  https://semanticdatacharter.com/ns/sdc4/TokenListSimpleType
sdc4:TokenListSimpleType rdf:type rdfs:Datatype ;
                         rdfs:comment "A simple type for a white space-delimited list of normalized token values." .


#################################################################
#    Object Properties
#################################################################

###  https://semanticdatacharter.com/ns/sdc4/acs
sdc4:acs rdf:type owl:ObjectProperty ;
         rdfs:domain sdc4:DMType ;
         rdfs:range sdc4:XdLinkType ;
         rdfs:comment "Identifier of an externally held access control system. This URI can be an ontology, vocabulary or descriptive document URI link." .


###  https://semanticdatacharter.com/ns/sdc4/hasAttestation
sdc4:hasAttestation rdf:type owl:ObjectProperty ;
                    rdfs:domain sdc4:DMType ;
                    rdfs:range sdc4:AttestationType ;
                    rdfs:comment "Attestation record of an instance of data." .


###  https://semanticdatacharter.com/ns/sdc4/hasAudit
sdc4:hasAudit rdf:type owl:ObjectProperty ;
              rdfs:domain sdc4:DMType ;
              rdfs:range sdc4:AuditType ;
              rdfs:comment "List of audits from the system of original commit of information forming the content, or from a conversion gateway which has processed this data." .


###  https://semanticdatacharter.com/ns/sdc4/hasCommitter
sdc4:hasCommitter rdf:type owl:ObjectProperty ;
                  rdfs:domain sdc4:AttestationType ;
                  rdfs:range sdc4:PartyType ;
                  rdfs:comment "Identity of person who committed the item." .


###  https://semanticdatacharter.com/ns/sdc4/hasDataItem
sdc4:hasDataItem rdf:type owl:ObjectProperty ;
                 rdfs:domain sdc4:DMType ;
                 rdfs:range sdc4:ClusterType ;
                 rdfs:comment "The data structure of the model." .


###  https://semanticdatacharter.com/ns/sdc4/hasExceptionalValue
sdc4:hasExceptionalValue rdf:type owl:ObjectProperty ;
                         rdfs:domain sdc4:XdAnyType ;
                         rdfs:range sdc4:ExceptionalValueType .


###  https://semanticdatacharter.com/ns/sdc4/hasFunction
sdc4:hasFunction rdf:type owl:ObjectProperty ;
                 rdfs:domain sdc4:ParticipationType ;
                 rdfs:range sdc4:XdStringType ;
                 rdfs:comment "The function of the performer in this participation. In some applications this might be called a Role." .


###  https://semanticdatacharter.com/ns/sdc4/hasInterval
sdc4:hasInterval rdf:type owl:ObjectProperty ;
                 rdfs:domain sdc4:ReferenceRangeType ;
                 rdfs:range sdc4:XdIntervalType ;
                 rdfs:comment "The data range for this reference range." .


###  https://semanticdatacharter.com/ns/sdc4/hasIntervalUnits
sdc4:hasIntervalUnits rdf:type owl:ObjectProperty ;
                      rdfs:domain sdc4:XdIntervalType ;
                      rdfs:range sdc4:InvlUnits .


###  https://semanticdatacharter.com/ns/sdc4/hasItems
sdc4:hasItems rdf:type owl:ObjectProperty ;
              rdfs:domain sdc4:ClusterType ;
              rdfs:range sdc4:ItemType ;
              rdfs:comment "List of ItemTypes which may be XdAdapterTypes and/or ClusterTypes." .


###  https://semanticdatacharter.com/ns/sdc4/hasLink
sdc4:hasLink rdf:type owl:ObjectProperty ;
             rdfs:domain sdc4:DMType ;
             rdfs:range sdc4:XdLinkType ;
             rdfs:comment "Optional link(s) to other locatable structures or external entities." .


###  https://semanticdatacharter.com/ns/sdc4/hasLocation
sdc4:hasLocation rdf:type owl:ObjectProperty ;
                 rdfs:domain sdc4:AuditType ;
                 rdfs:range sdc4:ClusterType ;
                 rdfs:comment "Location information of the particular site/facility within an organisation which handled the item." .


###  https://semanticdatacharter.com/ns/sdc4/hasLower
sdc4:hasLower rdf:type owl:ObjectProperty ;
              rdfs:domain sdc4:XdIntervalType ;
              rdfs:range sdc4:InvlType ;
              rdfs:comment "The lower boundary of the interval. Must be of the same datatype as the 'upper' boundary." .


###  https://semanticdatacharter.com/ns/sdc4/hasMagnitudeStatus
sdc4:hasMagnitudeStatus rdf:type owl:ObjectProperty ;
                        rdfs:domain sdc4:XdQuantifiedType ;
                        rdfs:range sdc4:MagnitudeStatus ;
                        rdfs:comment """An optional status that describes the relationship of this quantity's value to a contextual target or expected value.The target value itself is not defined here but is known from the context of the measurement, such as a process control limit, a clinical reference range, or a business rule.
Example: A temperature sensor reports a value of 140C. If the system's target operational temperature is 160C,  the magnitude-status for the 140C reading would be 'less_than'.""" .


###  https://semanticdatacharter.com/ns/sdc4/hasMode
sdc4:hasMode rdf:type owl:ObjectProperty ;
             rdfs:domain sdc4:ParticipationType ;
             rdfs:range sdc4:XdStringType ;
             rdfs:comment "The mode of the performer / activity interaction, e.g. present, by telephone, by email etc. If the participation is by device or software it may contain a protocol standard or interface definition." .


###  https://semanticdatacharter.com/ns/sdc4/hasParticipation
sdc4:hasParticipation rdf:type owl:ObjectProperty ;
                      rdfs:domain sdc4:DMType ;
                      rdfs:range sdc4:ParticipationType ;
                      rdfs:comment "List of other participations in the data." .


###  https://semanticdatacharter.com/ns/sdc4/hasPartyDetails
sdc4:hasPartyDetails rdf:type owl:ObjectProperty ;
                     rdfs:domain sdc4:PartyType ;
                     rdfs:range sdc4:ClusterType ;
                     rdfs:comment "A structured element for details about the party." .


###  https://semanticdatacharter.com/ns/sdc4/hasPartyRef
sdc4:hasPartyRef rdf:type owl:ObjectProperty ;
                 rdfs:domain sdc4:PartyType ;
                 rdfs:range sdc4:XdLinkType ;
                 rdfs:comment "Optional reference to more detailed demographic or identification information for this party, in an external system." .


###  https://semanticdatacharter.com/ns/sdc4/hasPerformer
sdc4:hasPerformer rdf:type owl:ObjectProperty ;
                  rdfs:domain sdc4:ParticipationType ;
                  rdfs:range sdc4:PartyType ;
                  rdfs:comment "The id and possibly demographic system link of the party that represents the primary provider of services in the activity." .


###  https://semanticdatacharter.com/ns/sdc4/hasProof
sdc4:hasProof rdf:type owl:ObjectProperty ;
              rdfs:domain sdc4:AttestationType ;
              rdfs:range sdc4:XdFileType ;
              rdfs:comment "Proof of attestation such as an GPG signature." .


###  https://semanticdatacharter.com/ns/sdc4/hasProvider
sdc4:hasProvider rdf:type owl:ObjectProperty ;
                 rdfs:domain sdc4:DMType ;
                 rdfs:range sdc4:PartyType ;
                 rdfs:comment "Optional identification of the source of the information, which might be: • a patient • a patient agent, e.g. parent, guardian • a clinician • a technician • a device or software" .


###  https://semanticdatacharter.com/ns/sdc4/hasReason
sdc4:hasReason rdf:type owl:ObjectProperty ;
               rdfs:domain sdc4:AttestationType ;
               rdfs:range sdc4:XdStringType ;
               rdfs:comment "Reason of this attestation. Usually coded from a standardized vocabulary." .


###  https://semanticdatacharter.com/ns/sdc4/hasReferenceRange
sdc4:hasReferenceRange rdf:type owl:ObjectProperty ;
                       rdfs:domain sdc4:XdOrderedType ;
                       rdfs:range sdc4:ReferenceRangeType ;
                       rdfs:comment "Optional list of ReferenceRanges for this value in its particular measurement context." .


###  https://semanticdatacharter.com/ns/sdc4/hasSubject
sdc4:hasSubject rdf:type owl:ObjectProperty ;
                rdfs:domain sdc4:DMType ;
                rdfs:range sdc4:PartyType ;
                rdfs:comment "Id of human subject of the data, e.g.: • subject of record (patient, customer, etc.) • a family member • another clinically relevant person." .


###  https://semanticdatacharter.com/ns/sdc4/hasSystemId
sdc4:hasSystemId rdf:type owl:ObjectProperty ;
                 rdfs:domain sdc4:AuditType ;
                 rdfs:range sdc4:XdStringType ;
                 rdfs:comment "Identifier of systems which created or handled the information item. 'Systems' can also be defined as an individual application or a data repository in which the data was manipulated." .


###  https://semanticdatacharter.com/ns/sdc4/hasSystemUser
sdc4:hasSystemUser rdf:type owl:ObjectProperty ;
                   rdfs:domain sdc4:AuditType ;
                   rdfs:range sdc4:PartyType ;
                   rdfs:comment "User(s) who created, committed, forwarded or otherwise handled the item." .


###  https://semanticdatacharter.com/ns/sdc4/hasUpper
sdc4:hasUpper rdf:type owl:ObjectProperty ;
              rdfs:domain sdc4:XdIntervalType ;
              rdfs:range sdc4:InvlType ;
              rdfs:comment "The upper boundary of the interval. Must be of the same datatype as the 'lower' boundary." .


###  https://semanticdatacharter.com/ns/sdc4/hasView
sdc4:hasView rdf:type owl:ObjectProperty ;
             rdfs:domain sdc4:AttestationType ;
             rdfs:range sdc4:XdFileType ;
             rdfs:comment "Optional visual representation of content attested e.g. screen image." .


###  https://semanticdatacharter.com/ns/sdc4/hasXdCountUnits
sdc4:hasXdCountUnits rdf:type owl:ObjectProperty ;
                     rdfs:domain sdc4:XdCountType ;
                     rdfs:range sdc4:XdStringType .


###  https://semanticdatacharter.com/ns/sdc4/hasXdDoubleUnits
sdc4:hasXdDoubleUnits rdf:type owl:ObjectProperty ;
                      rdfs:domain sdc4:XdDoubleType ;
                      rdfs:range sdc4:XdStringType .


###  https://semanticdatacharter.com/ns/sdc4/hasXdFloatUnits
sdc4:hasXdFloatUnits rdf:type owl:ObjectProperty ;
                     rdfs:domain sdc4:XdFloatType ;
                     rdfs:range sdc4:XdStringType ;
                     rdfs:comment "The name or type of the float value. May or may not come from a standardized vocabulary." .


###  https://semanticdatacharter.com/ns/sdc4/hasXdQuantityUnits
sdc4:hasXdQuantityUnits rdf:type owl:ObjectProperty ;
                        rdfs:domain sdc4:XdQuantityType ;
                        rdfs:range sdc4:XdStringType ;
                        rdfs:comment "The name or type of the quantity. Examples are \"kg\", \"mmHg\", \"ms-1\", \"km/h\". Should come from a standard terminology." .


###  https://semanticdatacharter.com/ns/sdc4/protocol
sdc4:protocol rdf:type owl:ObjectProperty ;
              rdfs:domain sdc4:DMType ;
              rdfs:range sdc4:XdStringType ;
              rdfs:comment "Optional external identifier of protocol used when collecting the data. This could be a clinical guideline, an operations protocol,etc." .


###  https://semanticdatacharter.com/ns/sdc4/workflow
sdc4:workflow rdf:type owl:ObjectProperty ;
              rdfs:domain sdc4:DMType ;
              rdfs:range sdc4:XdLinkType ;
              rdfs:comment "Identifier of externally held workflow engine (state machine) data for this workflow execution." .


###  https://semanticdatacharter.com/ns/sdc4/xddecimallist-units
sdc4:xddecimallist-units rdf:type owl:ObjectProperty ;
                         rdfs:domain sdc4:XdDecimalListType ;
                         rdfs:range sdc4:XdStringType ;
                         rdfs:comment "The name or type of the decimal list values. Examples are \"kg\", \"mmHg\", \"ms-1\", \"km/h\". Should come from a standard terminology. All values in the list share these units." .


###  https://semanticdatacharter.com/ns/sdc4/xddoublelist-units
sdc4:xddoublelist-units rdf:type owl:ObjectProperty ;
                        rdfs:domain sdc4:XdDoubleListType ;
                        rdfs:range sdc4:XdStringType ;
                        rdfs:comment "The name or type of the double list values. Examples are \"kg\", \"mmHg\", \"ms-1\", \"km/h\". Should come from a standard terminology. All values in the list share these units." .


###  https://semanticdatacharter.com/ns/sdc4/xdintegerlist-units
sdc4:xdintegerlist-units rdf:type owl:ObjectProperty ;
                         rdfs:domain sdc4:XdIntegerListType ;
                         rdfs:range sdc4:XdStringType ;
                         rdfs:comment "The name or type of the integer list values. Examples are \"items\", \"counts\", \"indices\". Should come from a standard terminology. All values in the list share these units." .


###  https://semanticdatacharter.com/ns/sdc4/xdnonnegativeintegerlist-units
sdc4:xdnonnegativeintegerlist-units rdf:type owl:ObjectProperty ;
                                    rdfs:domain sdc4:XdNonNegativeIntegerListType ;
                                    rdfs:range sdc4:XdStringType ;
                                    rdfs:comment "The name or type of the non-negative integer list values. Examples are \"items\", \"counts\", \"indices\". Should come from a standard terminology. All values in the list share these units." .


###  https://semanticdatacharter.com/ns/sdc4/xdpositiveintegerlist-units
sdc4:xdpositiveintegerlist-units rdf:type owl:ObjectProperty ;
                                 rdfs:domain sdc4:XdPositiveIntegerListType ;
                                 rdfs:range sdc4:XdStringType ;
                                 rdfs:comment "The name or type of the positive integer list values. Examples are \"items\", \"counts\", \"indices\". Should come from a standard terminology. All values in the list share these units." .


#################################################################
#    Data properties
#################################################################

###  https://semanticdatacharter.com/ns/sdc4/accuracy_margin
sdc4:accuracy_margin rdf:type owl:DatatypeProperty ;
                     rdfs:domain sdc4:XdQuantifiedType ;
                     rdfs:range xsd:decimal ;
                     rdfs:comment """Models ACCURACY. Defines the margin of systematic error, representing how close a measurement is to the true value. 
                This value is expressed as a plus-or-minus percentage of the magnitude. For example, a value of '2.5' indicates the true value is within +/- 2.5% of the stated magnitude.""" .


###  https://semanticdatacharter.com/ns/sdc4/act
sdc4:act rdf:type owl:DatatypeProperty ;
         rdfs:domain sdc4:XdAnyType ;
         rdfs:range xsd:string ;
         rdfs:comment """Access Control Tag: 

Descriptive name of this datatype model. Provides the semantic 'name' for the model. Replaces what would be a global XML element name or class name in conventional modeling."""@en-us ;
         rdfs:label "act" .


###  https://semanticdatacharter.com/ns/sdc4/alt-text
sdc4:alt-text rdf:type owl:DatatypeProperty ;
              rdfs:domain sdc4:XdFileType ;
              rdfs:range xsd:string ;
              rdfs:comment "Text to display in lieu of multimedia display or execution." .


###  https://semanticdatacharter.com/ns/sdc4/committed
sdc4:committed rdf:type owl:DatatypeProperty ;
               rdfs:domain sdc4:AttestationType ;
               rdfs:range xsd:dateTime ;
               rdfs:comment "Timestamp of committal of the item." .


###  https://semanticdatacharter.com/ns/sdc4/compression-type
sdc4:compression-type rdf:type owl:DatatypeProperty ;
                      rdfs:domain sdc4:XdFileType ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Compression/archiving mime-type. If this elements does not exist then it means there is no compression/archiving. For a list of common mime-types for compression/archiving see: http://en.wikipedia.org/wiki/List_of_archive_formats." .


###  https://semanticdatacharter.com/ns/sdc4/creation_timestamp
sdc4:creation_timestamp rdf:type owl:DatatypeProperty ;
                        rdfs:domain sdc4:DMType ;
                        rdfs:range xsd:dateTime ;
                        rdfs:comment """An optional run-time timestamp that indicates the exact moment this specific data instance was created or generated. 
            This element provides a definitive point-in-time reference for the data payload itself. 
            It is distinct from the design-time Dublin Core metadata which describes the model definition.
            This timestamp is crucial for data versioning, caching, and synchronization in distributed systems.""" .


###  https://semanticdatacharter.com/ns/sdc4/current-state
sdc4:current-state rdf:type owl:DatatypeProperty ;
                   rdfs:domain sdc4:DMType ;
                   rdfs:comment "The data structure of the model." .


###  https://semanticdatacharter.com/ns/sdc4/definition
sdc4:definition rdf:type owl:DatatypeProperty ;
                rdfs:domain sdc4:ReferenceRangeType ;
                rdfs:range xsd:string ;
                rdfs:comment "Term whose value indicates the meaning of this range, e.g. 'normal', 'critical', 'therapeutic' etc." .


###  https://semanticdatacharter.com/ns/sdc4/dm-encoding
sdc4:dm-encoding rdf:type owl:DatatypeProperty ;
                 rdfs:domain sdc4:DMType ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Name of character set encoding in which text values in this DM are encoded. Default is utf-8." .


###  https://semanticdatacharter.com/ns/sdc4/dm-language
sdc4:dm-language rdf:type owl:DatatypeProperty ;
                 rdfs:domain sdc4:DMType ;
                 rdfs:range xsd:language ;
                 rdfs:comment "Mandatory indicator of the localised language in which this DM is written. The value space of language is the set of all strings that are valid language identifiers as defined [RFC 3066]." .


###  https://semanticdatacharter.com/ns/sdc4/encoding
sdc4:encoding rdf:type owl:DatatypeProperty ;
              rdfs:domain sdc4:XdFileType ;
              rdfs:range xsd:short ;
              rdfs:comment "Name of character encoding scheme in which this value is encoded. Coded from the IANA charcater set table: http://www.iana.org/assignments/character-sets Unicode is the default assumption in S3Model, with UTF-8 being the assumed encoding. This                element allows for variations from these assumptions." .


###  https://semanticdatacharter.com/ns/sdc4/end
sdc4:end rdf:type owl:DatatypeProperty ;
         rdfs:domain sdc4:ParticipationType ;
         rdfs:range xsd:dateTime ;
         rdfs:comment "The ending datetime when the participation took place." .


###  https://semanticdatacharter.com/ns/sdc4/ev-name
sdc4:ev-name rdf:type owl:DatatypeProperty ;
             rdfs:domain sdc4:ExceptionalValueType ;
             rdfs:range xsd:string ;
             rdfs:comment "A short title or phase for the exceptional type value." .


###  https://semanticdatacharter.com/ns/sdc4/false-value
sdc4:false-value rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain sdc4:XdBooleanType ;
                 rdfs:range xsd:string ;
                 rdfs:comment "A string that represents a boolean False in the implementation. These are constrained by a set of enumerations in the model." ;
                 rdfs:label "false-value" .


###  https://semanticdatacharter.com/ns/sdc4/formalism
sdc4:formalism rdf:type owl:DatatypeProperty ;
               rdfs:domain sdc4:XdFileType ;
               rdfs:range xsd:string ;
               rdfs:comment "Name of the formalism or syntax used to inform an application regarding a candidate parser to use on the content. Examples might include: 'ATL', 'MOLA', 'QVT', 'GDL', 'GLIF', etc." .


###  https://semanticdatacharter.com/ns/sdc4/hash-function
sdc4:hash-function rdf:type owl:DatatypeProperty ;
                   rdfs:domain sdc4:XdFileType ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Hash function used to compute hash-result. See: http://en.wikipedia.org/wiki/List_of_hash_functions#Cryptographic_hash_functions" .


###  https://semanticdatacharter.com/ns/sdc4/hash-result
sdc4:hash-result rdf:type owl:DatatypeProperty ;
                 rdfs:domain sdc4:XdFileType ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Hash function result of the 'media-content'. There must be a corresponding hash function type listed for this to have any meaning. See: http://en.wikipedia.org/wiki/List_of_hash_functions#Cryptographic_hash_functions" .


###  https://semanticdatacharter.com/ns/sdc4/instance_id
sdc4:instance_id rdf:type owl:DatatypeProperty ;
                 rdfs:domain sdc4:DMType ;
                 rdfs:range xsd:string ;
                 rdfs:comment """An optional, globally unique identifier for this specific data instance. It is strongly recommended to use a UUID (Universally Unique Identifier).
            This ID provides a persistent identity for the data that is decoupled from any storage or transport mechanism (e.g., filename, database primary key). 
            It is essential for reliably tracking, updating, and referencing data in distributed systems, message queues, and data aggregation platforms.""" .


###  https://semanticdatacharter.com/ns/sdc4/instance_version
sdc4:instance_version rdf:type owl:DatatypeProperty ;
                      rdfs:domain sdc4:DMType ;
                      rdfs:range xsd:string ;
                      rdfs:comment """An optional identifier for the specific version of this data instance. 
            When paired with instance_id, it allows for a complete version history of a single data record.
            The type is a string to accommodate various versioning schemes, such as simple integers ('1', '2'), semantic versioning ('1.2.1'), or timestamps.""" .


###  https://semanticdatacharter.com/ns/sdc4/invl-date
sdc4:invl-date rdf:type owl:DatatypeProperty ;
               rdfs:domain sdc4:InvlType ;
               rdfs:range xsd:date .


###  https://semanticdatacharter.com/ns/sdc4/invl-dateTime
sdc4:invl-dateTime rdf:type owl:DatatypeProperty ;
                   rdfs:domain sdc4:InvlType ;
                   rdfs:range xsd:dateTime .


###  https://semanticdatacharter.com/ns/sdc4/invl-decimal
sdc4:invl-decimal rdf:type owl:DatatypeProperty ;
                  rdfs:domain sdc4:InvlType ;
                  rdfs:range xsd:decimal .


###  https://semanticdatacharter.com/ns/sdc4/invl-double
sdc4:invl-double rdf:type owl:DatatypeProperty ;
                 rdfs:domain sdc4:InvlType ;
                 rdfs:range xsd:double .


###  https://semanticdatacharter.com/ns/sdc4/invl-duration
sdc4:invl-duration rdf:type owl:DatatypeProperty ;
                   rdfs:domain sdc4:InvlType ;
                   rdfs:range xsd:duration .


###  https://semanticdatacharter.com/ns/sdc4/invl-float
sdc4:invl-float rdf:type owl:DatatypeProperty ;
                rdfs:domain sdc4:InvlType ;
                rdfs:range xsd:float .


###  https://semanticdatacharter.com/ns/sdc4/invl-int
sdc4:invl-int rdf:type owl:DatatypeProperty ;
              rdfs:domain sdc4:InvlType ;
              rdfs:range xsd:integer .


###  https://semanticdatacharter.com/ns/sdc4/invl-time
sdc4:invl-time rdf:type owl:DatatypeProperty ;
               rdfs:domain sdc4:InvlType ;
               rdfs:range xsd:time .


